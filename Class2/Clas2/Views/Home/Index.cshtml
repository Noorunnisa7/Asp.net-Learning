@{
    ViewBag.Title = "Home Page";
}


@{ 
    string name = "Noor Un Nisa";
    string companyName = "Connect Nisa";
    int number = 1121212;
    var numbers = new int[] { 1,2,3,4,5,6,7,8,9};
}



@{ 
    var names = new string[] { "Ali" ,"Raza" ,"Haider" ,"Saad"};
}


<h1>@ViewBag.data</h1>
<h1>@ViewBag.Title</h1>

<h1>Hello @name!</h1>
<p>Company Name: @companyName</p>
<p>Number: @number</p>

<ul>
    @foreach (var n in numbers)
     {
        <li>@n</li>
     }
</ul>

<ul>
    @foreach (var username in names)
    {
        <li>@username</li>
    }
    
</ul>